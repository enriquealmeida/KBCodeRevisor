Sub 'WriteData'

	/* Generated by DVelop Work With Plus Pattern [Start] - Do not change */

	For each
		order (CnsId) 
		using WWConsolidadoDS(&CliId, &KBId, &MTCnsEstId)

		// Write cell values
		&CellRow += 1
		
		Do 'BeforeWriteLine'
		&ExcelDocument.Cells(&CellRow, &FirstColumn + 0).Number = CnsId
		&ExcelDocument.Cells(&CellRow, &FirstColumn + 1).Text = MTCnsDsc
		&ExcelDocument.Cells(&CellRow, &FirstColumn + 2).Text = KBId
		&ExcelDocument.Cells(&CellRow, &FirstColumn + 3).Text = KBDsc
		&ExcelDocument.Cells(&CellRow, &FirstColumn + 4).Date = CnsFch
		&ExcelDocument.Cells(&CellRow, &FirstColumn + 5).Date = MTCnsFchRea
		&ExcelDocument.Cells(&CellRow, &FirstColumn + 6).Text = MTCnsEstId
		&ExcelDocument.Cells(&CellRow, &FirstColumn + 7).Text = MTCnsObs
		
		Do 'AfterWriteLine'

	Endfor

	/* Generated by DVelop Work With Plus Pattern [End] - Do not change */

EndSub

