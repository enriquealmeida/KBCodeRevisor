	/* Generated by DVelop Work With Plus Pattern [End] - Do not change */

	if &RspId.IsEmpty()
		TblSaldosLicencia.Visible = false
	else
		TblSaldosLicencia.Visible = true
		&RspSaldoLicenciaTomada = EmpleadoSaldoLicenciaTomada(&RspId)	
		&RspSaldoLicenciaLiquidada = EmpleadoSaldoLicenciaLiquidada(&RspId)
