Event 'DoExport'

	/* Generated by DVelop Work With Plus Pattern [Start] - Do not change */

	// Generate Excel spreadsheet and download it.
	MOD_Licencia.ExportWWLicencia.Call(&AusTpoId, &RspId, &LicEst, &LicFecIni, &LicFecIni_To, &ExcelFilename, &ErrorMessage)
